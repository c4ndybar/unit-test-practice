{
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "guard-for-in": [
      "off"
    ],
    "one-var": [
      "off"
    ],
    "require-jsdoc": [
      "off"
    ],
    "linebreak-style": [
      "off",
      "windows"
    ],
    "indent": [
      "warn",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-return-await": [
      "warn"
    ],
    "complexity": [
      "error",
      10
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "max-len": [
      "warn",
      {
        "code": 140,
        "ignoreComments": true
      }
    ],
    "arrow-spacing": [
      "error"
    ],
    "arrow-parens": [
      "warn"
    ],
    "no-var": [
      "warn"
    ],
    "prefer-const": [
      "warn"
    ]
  }
}
